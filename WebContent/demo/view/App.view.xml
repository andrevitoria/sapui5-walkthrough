<mvc:View controllerName="demo.walkthrough.controller.App"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
	<App>
		<pages>
			<Page title="{i18n>homePageTitle}">
			<headerContent>
					<Button
						icon="sap-icon://hello-world"
						press="onOpenDialog"/>
				</headerContent>
				
				<content>
					<mvc:XMLView viewName="demo.walkthrough.view.HelloPanel" />
					<mvc:XMLView viewName="demo.walkthrough.view.InvoiceList" />
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>